<?xml version="1.0"?>

<PropertyList>

	<path>PC-9M.ac</path>
	<offsets>
		<pitch-deg>-4.70</pitch-deg>   
		<heading-deg>0</heading-deg>
		<roll-deg>0</roll-deg>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>-0.13</z-m>
	</offsets>
	
	<model>
		<usage>interior</usage>
		<path>Aircraft/PC-9M/Models/Cockpit.xml</path>
	</model>
	
    <!-- Rembrandt auto switching for glass -->
	
    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <condition>
            <greater-than>
                <property>sim/multiplay/generic/float[6]</property>
                <value>9</value>
            </greater-than>
            <property>/sim/rendering/rembrandt/enabled</property>
       	 </condition>
		<object-name>canopy</object-name>
		<object-name>prop</object-name>
		<object-name>pdisk</object-name>
    </effect>
	
	<!-- Lights -->
	
	<model>
		<path>Aircraft/PC-9M/Models/Lights.xml</path>
	</model>
	
	<!-- Effects -->

	<model>
		<path>Aircraft/PC-9M/Models/Effects.xml</path>
	</model>
	
	<!-- Glass shader -->
    
    <animation>
        <type>shader</type>
        <shader>chrome</shader>
        <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
        <object-name>canopy</object-name>
    </animation>
	
	<animation>
		<type>noshadow</type>
        <object-name>canopy</object-name>
	</animation>
	
    <!-- Parts invisible from internally -->
    
    <animation>
        <type>select</type>
        <object-name>canopy</object-name>
		<condition>
            <not>
                <property>sim/current-view/internal</property>
            </not>
        </condition>
    </animation>
	
	<!-- MP sound -->
	
	<sound>
		<path>Aircraft/PC-9M/Sounds/MP-Sound.xml</path>
	</sound>

	<!-- Livery -->

	<nasal>
		<load>
			var livery_update = aircraft.livery_update.new("Aircraft/PC-9M/Models/Liveries");
		</load>
		<unload>
			livery_update.stop();
		</unload>
	</nasal>

