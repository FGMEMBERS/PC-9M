<?xml version="1.0"?>

<PropertyList>

<path>NWS.ac</path>

<animation> 
<type>select</type>
<object-name>nwsb</object-name>
<condition>
<equals>
<property>controls/gear/nose-wheel-steering</property>
<value>1</value>
</equals>
</condition>
</animation>

<animation> 
<type>material</type>
<object-name>nwsb1</object-name>
