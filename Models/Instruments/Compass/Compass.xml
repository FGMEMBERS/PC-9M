<?xml version="1.0"?>

<!-- by Petar jedvaj -->

<PropertyList>

<path>Compass.ac</path>

<animation>
<type>rotate</type>
<object-name>ring</object-name>
<property>instrumentation/magnetic-compass/indicated-heading-deg</property>
<center>
<x-m>0.00</x-m>
<y-m>0.00</y-m>
<z-m>0.00</z-m>
</center> 
<axis>
<x>0.0</x>
<y>0.0</y>
<z>1.0</z>
</axis>
</animation>

<animation> 
<type>material</type>
<object-name>ring</object-name>
<object-name>reticl</object-name>
<emission> 
<factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
<red>0.045</red>
<green>0.025</green>
<blue>0.015</blue>
</emission>
</animation>

<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>5</max-m>
</animation>

</PropertyList>