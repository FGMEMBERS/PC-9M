<?xml version="1.0"?>
<!-- torque pressure
Syd Adams
-->
<PropertyList>

    <path>T5.ac</path>
    
    <animation>
        <type>material</type>
        <object-name>T5face</object-name>
        <object-name>T5needle</object-name>
        <emission>
            <red>0.045</red>
            <green>0.025</green>
            <blue>0.015</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>T5needle</object-name>
        <property>engines/engine[0]/n1</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>110</ind><dep>150</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    
<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>5</max-m>
</animation>

</PropertyList>
